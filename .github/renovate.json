{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended",
		"helpers:pinGitHubActionDigests",
		"group:allNonMajor"
	],
	"labels": ["Dependencies"],
	"ignoreDeps": [],
	"npm": {
		"rangeStrategy": "bump",
		"packageRules": [
			{
				"matchUpdateTypes": ["minor", "patch"],
				"matchCurrentVersion": "!/^0/",
				"automerge": true
			},
			{
				"matchDepTypes": ["engines"],
				"enabled": false
			}
		]
	}
}
