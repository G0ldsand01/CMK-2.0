---
import Layout from './Layout.astro';
import { getSession } from 'auth-astro/server';
interface Props {
  title: string;
}

const { title } = Astro.props;
const session = await getSession(Astro.request);

if (!session) {
  return Astro.redirect('/login');
}
---

<Layout title={title} navbar={true}>
  <!--- Sidebar --->
  <slot />
</Layout>
